{
    "compilerOptions": {
      "target": "es2018",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": "./",
      "paths": {
        "*": [
          "node_modules/*",
          "types/*"
        ]
      }
    },
    "include": [
      "api/**/*.ts",
      "models/**/*.ts",
      "index.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  